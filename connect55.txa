Handle:Set:Connect:Mode             ROUTINE
!---
!--- This sets to the ClarioNET server
!---
 IF LOC:ISAPI_Mode
    UNHIDE(?LOC:ISAPI_Broker_Ver)
    IF LOC:ISAPI_Broker_Ver = '5.5'
      CLRNT_C.HostProgramDirectory = 'cws/c5launch.dll'   !ISAPI      Version
    ELSIF LOC:ISAPI_Broker_Ver = '5'
      CLRNT_C.HostProgramDirectory = 'cws/cwlaunch.dll'   !ISAPI      Version
    END
 ELSE
    HIDE(?LOC:ISAPI_Broker_Ver)
!    If Clip(Loc:LugarConectar)='Antyr Principal' Then
!        CLRNT_C.HostProgramDirectory = 'EXEC55/AutorentJR'               !EXE Broker Version
!    Elsif Clip(Loc:LugarConectar)='Antyr Secundario' Then
!        CLRNT_C.HostProgramDirectory = 'EXEC55/AutorentJR'               !EXE Broker Version
!    Elsif Clip(Loc:LugarConectar)='Antyr Sta. Rosa' Then
!        CLRNT_C.HostProgramDirectory = 'EXEC55/AutorentJR'               !EXE Broker Version
!    Elsif Clip(Loc:LugarConectar)='Antyr Emergente' Then
!        CLRNT_C.HostProgramDirectory = 'EXEC55/Arrendadoras/Antyr/AutorentJR'               !EXE Broker Version
!    Elsif Clip(Loc:LugarConectar)='Práctica Principal' Then
!        CLRNT_C.HostProgramDirectory = 'EXEC55/Practica'               !EXE Broker Version
!    Elsif Clip(Loc:LugarConectar)='Práctica Secundario' Then
!        CLRNT_C.HostProgramDirectory = 'EXEC55/Practica'               !EXE Broker Version
!    Elsif Clip(Loc:LugarConectar)='Práctica Sta. Rosa' Then
!        CLRNT_C.HostProgramDirectory = 'EXEC55/Practica'               !EXE Broker Version
!    Elsif Clip(Loc:LugarConectar)='Práctica Emergente' Then
!        CLRNT_C.HostProgramDirectory = 'EXEC55/Arrendadoras/Antyr/Practica'               !EXE Broker Version
!    End
 END
!---
 DISPLAY(?HostProgramDirectory)
 DISPLAY(?ServerPort          )
!---
 DO Handle:MakeFullProgramIDForDisplay
!---
 EXIT
!------------------------------------------------------------------------------
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
Handle:MakeFullProgramIDForDisplay          ROUTINE
!---
 FullProgramID = '/' & CLIP(CLRNT_C.HostProgramName) & '.0'
!---
 IF CLIP(CLRNT_C.HostProgramDirectory) <> ''
    FullProgramID = '/' & CLIP(CLRNT_C.HostProgramDirectory) & CLIP(FullProgramID)
 END
!---
 FullProgramID = 'http://' & CLIP(CLRNT_C.ServerName) & ':' & CLIP(CLRNT_C.ServerPort) & CLIP(FullProgramID)
!---
 DISPLAY(?FullProgramID)
!---
 EXIT
!------------------------------------------------------------------------------
[END]
