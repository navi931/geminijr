EMBED %ClarioNETClientProcedureCODE
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
!Analizar archivo y mostrarlo
Begin
    CASE MESSAGE('Archivo descargado y listo para Guardar o Leer','Descarga de Archivo',ICON:Question,'&Abrir|&Guardar|&Cancelar',2,0)

    OF 1                            !Abrir el archivo Bajado

        Glo:ArchivoMostrar=Clip(P2)&Clip(P3)&Clip(P4)&Clip(P5)&Clip(P6)
        If P1='PDF' Then Run(Clip(Glo:LectorPDF)&' "'&Clip(CLRNT_C.ClientTempDirectory)&Clip(P2)&Clip(P3)&Clip(P4)&Clip(P5)&Clip(P6)&'"')
        Elsif P1='CFD' Then Run(Clip(Glo:LectorCFD)&' "'&Clip(CLRNT_C.ClientTempDirectory)&Clip(P2)&Clip(P3)&Clip(P4)&Clip(P5)&Clip(P6)&'"')
        Elsif P1='TXT' Then Run(Clip(Glo:LectorTXT)&' "'&Clip(CLRNT_C.ClientTempDirectory)&Clip(P2)&Clip(P3)&Clip(P4)&Clip(P5)&Clip(P6)&'"')
        Elsif P1='CSV' Then Run(Clip(Glo:LectorCSV)&' "'&Clip(CLRNT_C.ClientTempDirectory)&Clip(P2)&Clip(P3)&Clip(P4)&Clip(P5)&Clip(P6)&'"')
        Elsif P1='XML' Then Run(Clip(Glo:LectorXML)&' "'&Clip(CLRNT_C.ClientTempDirectory)&Clip(P2)&Clip(P3)&Clip(P4)&Clip(P5)&Clip(P6)&'"')
        Elsif P1='DBF' Then Run(Clip(Glo:LectorDBF)&' "'&Clip(CLRNT_C.ClientTempDirectory)&Clip(P2)&Clip(P3)&Clip(P4)&Clip(P5)&Clip(P6)&'"')
        Else
            Message('No existe programa para abrir el tipo de archivo '&Clip(P1))
        End

    OF 2                            !Guardar el archivo bajado en la carpeta de Archivos Recibidos
        If Clip(Glo:ArchivosRecibidos)='' Then
            Glo:ArchivoMostrar=Clip(P2)&Clip(P3)&Clip(P4)&Clip(P5)&Clip(P6)
            Glo:ArchivosRecibidos=Clip(CLRNT_C.ClientTempDirectory)
        Else
            Glo:ArchivoMostrar=Upper(Clip(P2)&Clip(P3)&Clip(P4)&Clip(P5)&Clip(P6))
            If P1='CFD' And Sub(Glo:ArchivoMostrar,1,4)='ACFD' Then
                Glo:ArchivoMostrar=Sub(Clip(Glo:ArchivoMostrar),5,Instring('-F.XML',Clip(Glo:ArchivoMostrar),1,1)-5)&'.XML'
            End
            Copy(Clip(CLRNT_C.ClientTempDirectory)&Clip(P2)&Clip(P3)&Clip(P4)&Clip(P5)&Clip(P6),Clip(Glo:ArchivosRecibidos)&'\'&Clip(Glo:ArchivoMostrar))
        End
        Message('Se guardó el archivo en tu máquina en la carpeta: '&Clip(Glo:ArchivosRecibidos)&'\'&Clip(Glo:ArchivoMostrar))
    OF 3
        !No ejecutar acción
    END
End
[END]
EMBED %ClarioNETSendClientFilesToServerCODE
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
Begin
    FREE(FileListQueue)
    RetVal# = FILEDIALOG('Selecciona los archivos a enviar (Locales)', FileList, Extension,11000b)
    IF RetVal# = 0 THEN
        RETURN('No seleccionaste archivos para enviar')
    Else
        !Message('Archivos '&FileList)
    END
    FileList2=FileList
    Pos1 = INSTRING('|', FileList2, 1, 1)
    !Si solo es 1 archivo
    IF Pos1 = 0
        FLQ:Filename = CLIP(FileList2)
        ADD(FileListQueue)
    ELSE
        PathName = FileList2[1 : Pos1-1] & '\'
        FileList2 = FileList2[Pos1+1 : Len(Left(Filelist2))-Pos1]
        !Message('Path = '&PathName&'|resto: '&Clip(Filelist2))
        Loop
            POS2 = INSTRING('|', FileList2, 1, 1)
            IF POS2=0 Then
                FLQ:Filename = PathName & Clip(FileList2)
                ADD(FileListQueue)
                Break
            Else
                FLQ:Filename = PathName & Sub(FileList2,1, Pos2 - 1)
                ADD(FileListQueue)
                FileList2=Sub(FileList2,Pos2+1,Len(Left(FileList2))-Pos2)
            End
        End
    END
    !This code just to displays the queue
    LOOP I#=1 TO RECORDS(FileListQueue)
        GET(FileListQueue, I#)
        If Instring('XML',Upper(FLQ:Filename),1,1)=0 And Instring('PDF',Upper(FLQ:Filename),1,1)=0 Then Delete(FileListQueue).
    END
    RETURN('ok')
End
[END]
