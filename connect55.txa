Handle:Button:Connect       ROUTINE
 DATA
LOC:ReturnVal     LONG
 CODE
!---
 SETCURSOR(CURSOR:Wait)
 0{PROP:Timer} = 0
 Hide(?Image1)
 Unhide(?Image2)
 Unhide(?Image3)
 !HIDE(0)                                                   ! REQUIRED  (quitada por JR)
 !
 !
 !CLRNT_C.PingFlag = 1      (ClarioNET:StartServerProgram == Returns 99)
 !
 !
 LOC:ReturnVal = ClarioNET:StartServerProgram()            ! REQUIRED
 SETCURSOR()
 IF LOC:ReturnVal
    CASE LOC:ReturnVal
     OF CLRNT_ERR_NO_CREATE_TEMP
        MESSAGE('No puedo crear archivos temporales'                 ,'Error de sistema',ICON:Exclamation)
     OF CLRNT_ERR_HTTP_FAILURE       
        MESSAGE('No puedo abrir una sesión de internet'              ,'Error de sistema',ICON:Exclamation)
     OF CLRNT_ERR_SESSION_FAILURE    
        MESSAGE('No me puedo conectar al programa del server'        ,'Error de sistema',ICON:Exclamation)
     OF CLRNT_ERR_DECOMP_INIT_FAILURE
        MESSAGE('No puedo inicializar la descompresión del programa' ,'Error de sistema',ICON:Exclamation)
     OF CLRNT_ERR_NO_HOSTNAME
        MESSAGE('No ha especificado el nombre del SITIO a dirigirse' ,'Error de sistema',ICON:Exclamation)
     OF CLRNT_ERR_NO_HOSTDIR
        MESSAGE('No hay nombre de SITIO especificado'                ,'Error de sistema',ICON:Exclamation)
     OF CLRNT_ERR_NO_SERVERMAME
        MESSAGE('No se ha especificado el Servidor de Nombres'       ,'Error de sistema',ICON:Exclamation)
     OF CLRNT_ERR_VERSION_MISMATCH
        MESSAGE('No coincide la versión'                             ,'Error de sistema',ICON:Exclamation)
   ELSE
        MESSAGE('Ha ocurrido un error indeterminado [ ' & LOC:ReturnVal & ' ]','Error de sistema',ICON:Exclamation)
   END
   Hide(?Image2)
   Hide(?Image3)
   Unhide(?Image1)
   UNHIDE(0); DISPLAY; EXIT
 END
 !----------------------------------------------------------------------------------------------
 ! At this time CLRNT_C.ServerString1 -> 3 is available for inspection
 ! Also, CLRNT_C.HostSessionID has been set.
 !----------------------------------------------------------------------------------------------
 LOC:ReturnVal = ClarioNET:StartClientSession()                  ! REQUIRED
 CASE LOC:ReturnVal
   OF CLRNT_ERR_NONE
   OF CLRNT_ERR_INACTIVE
      MESSAGE('Se detecto inactividad y el cliente se desconecto automáticamente'      ,'Error de sistema',ICON:Exclamation)
   OF CLRNT_ERR_HTTP_TIMEOUT
      MESSAGE('Error de tiempo de espera en HTTP'                                      ,'Error de sistema',ICON:Exclamation)
 ELSE
      MESSAGE('Ha ocurrido un error indeterminado [ ' & LOC:ReturnVal & ' ]'  ,'Error de sistema',ICON:Exclamation)
 END
 POST(EVENT:Accepted,?Exit)
 UNHIDE(0)                                                 ! REQUIRED
 DISPLAY
!---
 EXIT
